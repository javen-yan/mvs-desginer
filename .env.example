# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=true
SECRET_KEY=your-secret-key-here
HOST=0.0.0.0
PORT=5000

# Database Configuration
DATABASE_URL=postgresql://mvs_user:mvs_password@localhost:5432/mvs_designer
DEV_DATABASE_URL=postgresql://mvs_user:mvs_password@localhost:5432/mvs_designer_dev

# JWT Configuration
JWT_SECRET_KEY=your-jwt-secret-key-here

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=us-east-1
S3_BUCKET_NAME=your-s3-bucket-name
S3_USE_SSL=true

# Meshroom Configuration
MESHROOM_PATH=/opt/Meshroom
MESHROOM_CACHE_DIR=/tmp/meshroom_cache